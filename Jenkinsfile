// Declarative pipeline syntax example

pipeline {
    agent any

    stages{

        stage("checkout"){

            steps {

                echo 'git checkout ..... !!'

            } // steps ends

        } // stage ends

            stage("build"){

                    steps {

                        echo ' building thee code ...... !!'

                    } // steps ends

                } // stage ends

    } // stages end


} // pipeline ends
